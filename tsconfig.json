{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "types": [
         "koa-body"
      ],
      "baseUrl": ".",
      "paths": {
         "@/*": [
            "src/*"
         ]
      }
   }
}
